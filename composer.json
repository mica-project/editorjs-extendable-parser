{
  "name": "mica-project/editorjs-extendable-parser",
  "type": "library",
  "description": "An extendable server-side bidirectional parser for EditorJS structures",
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "MicaProject\\EJSParser\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MicaProject\\EJSParser\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit tests/ --coverage-clover=coverage.xml"
  },
  "require": {
    "php": "^8.3",
    "ext-dom": "*",
    "ext-json": "*",
    "masterminds/html5": "^2.7@dev",
    "ext-libxml": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^11"
  },
  "license": "MIT"
}
